{
  "name": "gazprom-analytical-system",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "moment": "^2.24.0",
    "npm": "^6.8.0",
    "react": "^16.11",
    "react-dom": "^16.11",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "react-vis": "^1.11.6",
    "stylus": "^0.54.5"
  },
  "scripts": {
    "build-css": "stylus -c src/stylus/ --out src/css",
    "watch-css": "npm run build-css && stylus -c -w src/stylus/ --out src/css",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
